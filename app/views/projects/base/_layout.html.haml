= render 'submenu'
= render 'repo_block', project: @project
= render 'about_block', project: @project

- if @commit.present?
  %h3= t("layout.projects.last_commit")
  - GitPresenters::CommitAsMessagePresenter.present(@commit, project: @project) do |presenter|
